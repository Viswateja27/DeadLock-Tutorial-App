<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main13"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="@drawable/eega"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.viswatejayetukuri.deadlock.Main13Activity">


    <Button
        android:text="Deadlock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="Video1"
        android:background="#0b868b"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/button20"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="Banker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="Video3"
        android:layout_below="@+id/button21"
        android:background="#0b868b"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp"
        android:id="@+id/button22"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="wait-for graph"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:background="#0b868b"
        android:onClick="Video2"
        android:id="@+id/button21"
        android:layout_below="@+id/button20"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />

    <Button
        android:text="detection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button22"
        android:background="#0b868b"
        android:onClick="Video4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="31dp"
        android:id="@+id/button23"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />
</RelativeLayout>
